{
  "name": "@p1n2o/utils",
  "description": "A collection of utility functions for common JavaScript/TypeScript tasks",
  "version": "0.1.1",
  "author": {
    "name": "Manuel Pinto",
    "email": "manuel@pinto.dev"
  },
  "tasks": {
    "start": "deno run --watch -A playground.ts",
    "format": "deno fmt",
    "lint": "deno lint"
  },
  "imports": {
    "@commitlint/cli": "npm:@commitlint/cli@^19.6.1",
    "@commitlint/config-conventional": "npm:@commitlint/config-conventional@^19.6.0",
    "@p1n2o/utils": "./mod.ts",
    "lefthook": "npm:lefthook@^1.10.10"
  },
  "exports": {
    ".": "./mod.ts",
    "./user-agent": "./lib/user-agent/main.ts"
  },
  "publish": {
    "exclude": [
      "node_modules",
      ".git",
      ".github",
      ".cache",
      ".vscode",
      "playground.ts"
    ]
  },
  "compilerOptions": {
    "strict": true
  },
  "nodeModulesDir": "auto"
}
